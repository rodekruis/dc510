{
  "name": "@keystonejs/example-projects-starter",
  "description": "An example KeystoneJS project with a User list and Authentication.",
  "private": true,
  "version": "5.0.0",
  "author": "The KeystoneJS Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "dev": "nodemon --exec 'knex migrate:up && keystone dev'",
    "build": "keystone build",
    "start": "knex migrate:up && keystone start"
  },
  "dependencies": {
    "@keystonejs/adapter-knex": "~5.1.0",
    "@keystonejs/app-admin-ui": "^5.2.0",
    "@keystonejs/app-graphql": "^5.0.0",
    "@keystonejs/auth-password": "^5.0.0",
    "@keystonejs/fields": "^5.2.0",
    "@keystonejs/fields-datetime-utc": "~5.0.0",
    "@keystonejs/fields-wysiwyg-tinymce": "~5.0.1",
    "@keystonejs/keystone": "^5.2.1",
    "@keystonejs/list-plugins": "~5.0.0",
    "cross-env": "^6.0.3",
    "dotenv": "~8.2.0"
  },
  "devDependencies": {
    "babel-eslint": "~10.0.3",
    "eslint": "~6.6.0",
    "eslint-config-prettier": "~6.7.0",
    "eslint-plugin-prettier": "~3.1.1",
    "husky": "~3.1.0",
    "lint-staged": "~9.4.3",
    "nodemon": "~2.0.0",
    "npm-check-updates": "~3.2.1",
    "prettier": "~1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --ignore-pattern '!.eslintrc.js' --fix",
      "git add"
    ]
  }
}
